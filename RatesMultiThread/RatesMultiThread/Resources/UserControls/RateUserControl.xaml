<UserControl x:Class="RatesMultiThread.Resources.UserControls.RateUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RatesMultiThread.Resources.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="175" d:DesignWidth="800">
    <Grid>
        <Viewbox StretchDirection="Both" Stretch="Uniform">
            <Grid Width="800">
                <local:RateDataUserControl x:Name="Rater"/>
                <local:RateSetupUserControl x:Name="RaterSetup"/>
            </Grid>
        </Viewbox>
    </Grid>
</UserControl>
